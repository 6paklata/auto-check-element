    "eslint": "^4.19.1",
    "eslint-plugin-github": "^0.24.1",
    "flow-bin": "^0.69.0",
    "gh-pages": "^1.1.0",
    "karma": "^2.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
